{
    "extends": [
        "airbnb-base/legacy"
    ],
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-console": 0,
        "func-names": 0,
        "quotes": 0,
        "max-len": [
            2,
            120,
            2
        ],
        "vars-on-top": 0,
        "no-unused-expressions": 0,
        "space-before-function-paren": 0
    }
}
